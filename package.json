    "eject": "react-scripts eject",
    "eslint": "eslint --ext js .",
    "i18n": "node scripts/update_i18n.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
